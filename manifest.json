{
    "name": "LogRun Extension",
    "version": "1.3",
    "manifest_version": 2,
    "description": "The first testing version",
    "intents": {
        "http://drive.google.com/intents/opendrivedoc": [
            {
                "type": ["application/vnd.google.drive.ext-type.lvz"],
                "href": "main.html",
                "title" : "Open",
                "disposition" : "window"
            }
        ]
    },

    "icons" : {
        "16" : "lvz.jpg",
        "128" : "lvz1.jpg"
    },
    "app" : {

        "launch" : {
            "local_path" : "main.html"
        }
    },
    "permissions": [
    ]
}